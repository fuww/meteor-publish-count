{
  "extends": "google",
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "require-jsdoc": 0,
    "quotes": [2, "single"],
    "quote-props": [2, "as-needed"],
    "camelcase": [2, {"properties": "never"}],
    "no-console": 2,
    "no-unused-vars": [2, {"vars": "local", "args": "after-used"}],
    "no-var": 2,
    "prefer-const": 2,
    "no-new-object": 2,
    "object-shorthand": 2,
    "no-array-constructor": 2,
    "array-callback-return": 2,
    "prefer-template": 2,
    "template-curly-spacing": 2,
    "no-useless-escape": 2,
    "no-loop-func": 2,
    "prefer-rest-params": 2,
    "no-param-reassign": 2,
    "prefer-arrow-callback": 2,
    "arrow-spacing": 2,
    "arrow-parens": 2,
    "arrow-body-style": 2,
    "no-confusing-arrow": 2,
    "no-useless-constructor": 2,
    "no-dupe-class-members": 2,
    "no-duplicate-imports": 2,
    "no-iterator": 2,
    "no-case-declarations": 2,
    "no-nested-ternary": 2,
    "no-unneeded-ternary": 2,
    "brace-style": 2,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "comma-dangle": [2, "never"],
    "keyword-spacing": 2,
    "no-invalid-this": 0,
    "max-len": [1, 80],
    "no-undef": [2]
  },
  "globals": {
    "Package": false,
    "Npm": false
  },
  "plugins": [
    "react"
  ]
}
